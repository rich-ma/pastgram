{
  "name": "pastgram",
  "version": "1.0.0",
  "description": "MERN stack Instagram clone",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon --ignore app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rich-ma/pastgram.git"
  },
  "author": "Richard Ma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rich-ma/pastgram/issues"
  },
  "homepage": "https://github.com/rich-ma/pastgram#readme",
  "dependencies": {
    "aws-sdk": "^2.458.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.5.4",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
